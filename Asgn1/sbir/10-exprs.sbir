;;   2: #
;;   3: # All of the following should print something without error messages.
;;   4: # This program checks to see if expressions can be interpreted.
;;   5: #
;;   6: 
;;   7: 	let pi = 4 * atan(1)
;;   8: 	let e = exp(1)
;;   9: 
;;  10: 	print "1+1      = ", 1+1
;;  11: 	print "2-2      = ", 2- 2
;;  12: 	print "3*3      = ", 3*3
;;  13: 	print "4/9      = ", 4/9
;;  14: 	print "2^10     = ", 2^10
;;  15: 	print "3*4+5*6  = ", 3*4+5*6
;;  16: #
;;  17: 	print "log(10)  = ", log(10)
;;  18: 	print "sqrt(2)  = ", sqrt(2)
;;  19: 	print "pi       = ", pi
;;  20: 	print "e        = ", e
;;  21: #
;;  22: 	print "+1/+0    = ", +1/+0
;;  23: 	print "-1/+0    = ", -1/+0
;;  24: 	print "+1/-0    = ", +1/-0
;;  25: 	print "-1/-0    = ", -1/-0
;;  26: 	print "+0/+0    = ", +0/+0
;;  27: 	print "-0/-0    = ", -0/-0
;;  28: 	print "sqrt(-1) = ", sqrt(-1)
;;  29: 	print "log(0)   = ", log(0)
;;  30: #
;;  31: 	print "6.02e23  = ", 6.02*10^23
;;  32: 	print "(1+2)/7  = ", (1+2)/7
(
(    1          )
(    2          )
(    3          )
(    4          )
(    5          )
(    6          )
(    7          (let pi (* 4 (atan 1))))
(    8          (let e (exp 1)))
(    9          )
(   10          (print "1+1      = " (+ 1 1)))
(   11          (print "2-2      = " (- 2 2)))
(   12          (print "3*3      = " (* 3 3)))
(   13          (print "4/9      = " (/ 4 9)))
(   14          (print "2^10     = " (^ 2 10)))
(   15          (print "3*4+5*6  = " (+ (* 3 4) (* 5 6))))
(   16          )
(   17          (print "log(10)  = " (log 10)))
(   18          (print "sqrt(2)  = " (sqrt 2)))
(   19          (print "pi       = " pi))
(   20          (print "e        = " e))
(   21          )
(   22          (print "+1/+0    = " (/ (+ 1) (+ 0))))
(   23          (print "-1/+0    = " (/ (- 1) (+ 0))))
(   24          (print "+1/-0    = " (/ (+ 1) (- 0))))
(   25          (print "-1/-0    = " (/ (- 1) (- 0))))
(   26          (print "+0/+0    = " (/ (+ 0) (+ 0))))
(   27          (print "-0/-0    = " (/ (- 0) (- 0))))
(   28          (print "sqrt(-1) = " (sqrt (- 1))))
(   29          (print "log(0)   = " (log 0)))
(   30          )
(   31          (print "6.02e23  = " (* 6.02 (^ 10 23))))
(   32          (print "(1+2)/7  = " (/ (+ 1 2) 7)))
)
